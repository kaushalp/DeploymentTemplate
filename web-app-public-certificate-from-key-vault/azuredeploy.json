{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "certificateBlob": {
            "type": "String"
        }
    },
    "variables": {
        "publicCertificateName": "[concat(parameters('kaushal'), '-publiccert')]"
    },
    "resources": [
        {
            "type": "Microsoft.Web/sites/publicCertificates",
            "name": "[concat(parameters('kaushal'), '/', variables('publicCertificateName'))]",
            "apiVersion": "2016-03-01",
            "location": "[resourceGroup().location]",
            "properties": {
                "publicCertificateLocation": "LocalMachineMy",
                "blob": "[parameters('certificateBlob')]",
            }
        }
    ],
	"outputs": {
		"certificateBlob": {
			"type": "String"
			"value": "[parameters('certificateBlob')]"
		}
	}
}
